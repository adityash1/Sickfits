    "dotenv": "^16.0.3",
    "next": "^13.1.6",
    "nodemailer": "^6.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stripe": "^11.9.1",
    "typescript": "^4.8.0"
